/* pages/Withdrawal/Withdrawal.wxss */
.local {
  width: 100vw;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.central {
  width: 95%;
  height: 100%;
  background: #fff;
}

.top {
  display: flex;
  flex-direction: column;
}

.money {
  width: 100%;
  margin-left: 15rpx;
  display: flex;
  align-items: center;
  height: 120rpx;
  font-size: 30rpx;
  color: #333333;
  font-weight: 600;
}

.t-money {
  display: flex;
}
.ipt{
  margin-top: 40rpx;
  color: #999999;
  font-size: 28rpx;
}

.yang-money {
  font-size: 72rpx;

}
.box-tow{
  display: flex;
  font-size: 28rpx;
  width: 100%;
  color: #333333;
  font-weight: 500;
  justify-content: space-between;
}
.box-one{
  width: 50%;
  display: flex;
  justify-content: space-between;
}
.cor{
  color: #61CF8C;
}
.money-from{
  margin-top: 40rpx;
  width: 680rpx !important;
  height: 70rpx;
  background-color: #61CF8CFF;
  border-radius:40rpx ;
  color: #ffffff;
  line-height: 35rpx;
  font-size: 30rpx;
}
.choice{
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  color: #333333;
  font-weight: 700;
}
.cancel{
  position: absolute;
  left: 20rpx;
  color: #999999;
  font-size: 24rpx;
}
.img{
  width: 48rpx;
  height: 48rpx;
}
.branch{
  margin-top: 38rpx;
  width: 100%;
  height: 60rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.bord{
  border-bottom: solid 0.2rpx #EEEEEE;
}
.gonicon{
  width: 48rpx;
  height: 48rpx;
}
.bank{
  padding: 0 15rpx 0 15rpx;
}
.card{
  display: flex;
}
.immediately{
  width: 95%;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 0;
  background: #FFFFFF;
}
.login-bu{
  width: 600rpx !important;
  height: 70rpx;
  background-color: #61CF8CFF;
  border-radius:40rpx ;
  color: #ffffff;
  line-height: 35rpx;
  font-size: 30rpx;
}
.Arrivalicon{
  width: 36rpx;
  height: 36rpx;
}
.youicon{
  width: 32rpx;
  height: 32rpx;
}
.bottom{
  height: 100rpx;
}