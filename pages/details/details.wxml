          <van-goods-action-icon icon="chat-o" text="收藏" bind:click="onClickIcon" />
          <van-goods-action-button text="立即预约" bind:click="onClickButton" />
        </van-goods-action>
      </view>
      <van-popup show="{{ show }}" position="bottom" custom-style="height: 90%;" bind:close="onClose">
        <view class="local">
          <view class="central-section">
            <view class="fill-in">填写预约信息</view>
            <view class="contacts">
              <view class="contact" bindtap="goaddress">
                <view class="Contact-address">请选择联系地址</view>
                <view class="picture">
                  <image src="../../img/mipmap-mdpi/xiangyou.png" class="immm-g"></image>
                </view>
              </view>
              <view class="contact">
                <view class="Contact-address">请选择联系地址</view>
                <view class="picture">
                  <image src="../../img/mipmap-mdpi/xiangyou.png" class="immm-g"></image>
                </view>
              </view>
              <view class="contact">
                <view class="Contact-address">请选择联系地址</view>
                <view class="picture">
                  <image src="../../img/mipmap-mdpi/xiangyou.png" class="immm-g"></image>
                </view>
              </view>
            </view>
            <view class="Servicetype">
                <view class="service-sp">服务类型</view>
                <view class="Escort-lp">
                  <view wx:for="{{arr}}" wx:key="item" class="yi-hospital">
                    <view class="yi-Escort">{{item.name}}</view>
                  </view>
                </view>
              <view class="service-sp">
                <view>服务项目</view>
                <view class="Escort-lp">
