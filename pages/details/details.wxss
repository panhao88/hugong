.local {
  width: 100vw;
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  background: #FAFAFAFF;
}

.central {
  width: 95%;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
  bottom: 60rpx;
}

.region {
  width: 100%;
  border-radius: 20rpx;
}

.region-tow {
  padding-left: 20rpx;
