/* 联系地址左划删除 */
.dele {
  width: 100%;
  overflow: hidden;
  box-sizing: border-box;
  padding-bottom: 10rpx;
}

.touch-item {
  font-size: 28rpx;
  display: flex;
  /* //均匀排布每个元素 */
  justify-content: space-between;
  border-bottom: 1px solid #ccc;
  width: 100%;
  overflow: hidden;
}

.content {
  display: flex;
  width: 100%;
  line-height: 44rpx;
  margin-right: 0;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(180rpx);
  transform: translateX(180rpx);
  margin-left: -180rpx;
}
.del {
  background-color: #FF2C2C;
  font-size: 28rpx;
  width: 240rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  -webkit-transform: translateX(90px);
  transform: translateX(180rpx);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}

.touch-move-active .content,
.touch-move-active .del,
.touch-move-active .plcontent {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}
/* 服务对象左划删除 */
.touch-move-activeone .content,
.touch-move-activeone .del,
.touch-move-activeone .plcontent {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}