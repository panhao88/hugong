  width: 60rpx;
  margin: 0 auto;
  text-align: center;
  height: 60rpx;
  border-radius: 50%;
  margin: 22rpx;
  font-size: 26rpx;
  color: #333333;
  font-weight: 700;
  display: flex;
  float: left;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-bottom: #FFFFFF 1px solid;
  box-sizing: border-box;
}

.calendar .week .saturday {
  color: #000;
}

.calendar .week .sunday {
  color: #000;
}

/* 当天的时间 */
.calendar .week .now {
  border: none;
  background: #F5F5F5;
  color: #888888;
  border-radius: 50%;
  box-sizing: border-box;
}

/* 选中的时间 */
.calendar .week .selected {
  border: none;
  background: #61CF8C !important;
  color: #FFFFFF !important;
  border-radius: 50%;
  box-sizing: border-box;
}

/* 过去时间 */
.calendar .week .disabled {
  border: none;
  background: #F5F5F5;
  text-decoration: line-through;
  border: #FFFFFF 1px solid;
  color: #888888;
  box-sizing: border-box;
}

/* 为空白时间 */
.calendar .week .empty {
  border: none;
  /* background: #F5F5F5; */
  background: #FFFFFF !important;
  box-sizing: border-box;
}

/* 有天数的背景 */
.calendar .week .aaaa {
  border: none;
  /* background: #F5F5F5; */
  background: #F5F5F5;
  border: #FFFFFF 1px solid;
  color: #888888;
  box-sizing: border-box;
}

