  background: #F1F1F1;
}

.guarantee {
  display: flex;
  flex-direction: column;
}

.security {
  height: 100rpx;
  display: flex;
  align-items: center;
  margin-left: 20rpx;
}

.Allcertifications {
  display: flex;
  justify-content: space-around;
  font-size: 28rpx;
}

.vertical {
  display: flex;
